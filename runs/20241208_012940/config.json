{
  "data": {
    "path": "data/processed/training_data.npz",
    "grid_size": [
      6,
      7
    ],
    "max_objects": 30
  },
  "model": {
    "latent_dim": 32,
    "hidden_dims": [
      256,
      128
    ],
    "attention_heads": 4,
    "dropout": 0.1,
    "beta_start": 0.0,
    "beta_end": 1.5,
    "beta_steps": 2000
  },
  "training": {
    "num_epochs": 100,
    "batch_size": 32,
    "learning_rate": 0.0001,
    "weight_decay": 1e-05,
    "grad_clip": 1.0,
    "val_split": 0.1,
    "patience": 15,
    "checkpoint_interval": 5,
    "sample_interval": 10,
    "num_workers": 0
  }
}